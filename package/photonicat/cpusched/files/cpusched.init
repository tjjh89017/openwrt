#!/bin/sh /etc/rc.common

USE_PROCD=1
START=99

start_service() {
	procd_open_instance
	procd_set_param command /usr/bin/cpusched.sh
	procd_set_param respawn "${respawn_threshold:-3600}" "${respawn_timeout:-5}" "${respawn_retry:-5}"
	procd_set_param pidfile "/var/run/cpusched.pid"
	procd_close_instance
}
