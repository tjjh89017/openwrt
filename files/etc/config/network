
config interface 'loopback'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'
	option device 'lo'

config globals 'globals'
	option ula_prefix 'fd1e:7270:ea04::/48'

config interface 'lan'
	option proto 'static'
	option netmask '255.255.248.0'
	option ip6assign '60'
	option ipaddr '172.16.0.1'
	option device 'br-lan'

config interface 'wan'
	option proto 'dhcp'
	option metric '1'
	option device 'eth0'

config interface 'wan6'
	option proto 'dhcpv6'
	option metric '2'
	option device 'eth0'

config interface 'VPN'
	option proto 'static'
	option ipaddr '10.10.10.1'
	option netmask '255.255.255.0'
	option device 'ipsec0'

config interface 'vpn0'
	option proto 'none'
	option device 'tun0'

config interface 'wwan_5g'
	option proto 'dhcp'
	option metric '10'
	option auto '0'
	option device 'usb0'

config interface 'wwan_5g_v6'
	option proto 'dhcpv6'
	option metric '11'
	option auto '0'
	option device 'usb0'

config interface 'wwan_lte'
	option proto 'dhcp'
	option metric '12'
	option device 'wwan0'
	option auto '0'

config interface 'wwan_lte_v6'
	option proto 'dhcpv6'
	option metric '13'
	option device 'wwan0'
	option auto '0'

config device
	option name 'br-lan'
	option type 'bridge'
	list ports 'eth1'

config interface 'wwan'
	option proto 'modemmanager'
	option device '/sys/devices/platform/usbhost/fd000000.dwc3/xhci-hcd.5.auto/usb8/8-1'
	option apn 'internet'
	option auth 'none'
	option iptype 'ipv4v6'
	option metric '513'
	option sourcefilter '0'
